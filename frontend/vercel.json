{
  "rewrites": [
    {
      "source": "/",
      "destination": "https://retrobank-backend.up.railway.app/api/login"
    },
    {
      "source": "/:path*",
      "destination": "https://retrobank-backend.up.railway.app/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
